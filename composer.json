{
  "name": "phico/cdn",
  "description": "Lightweight CDN support for Phico",
  "type": "library",
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Phico\\Cdn\\": "src/"
    }
  },
  "authors": [
    {
      "name": "indgy",
      "email": "me@indgy.uk"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "php": "^8.0",
    "phico/http-client": "dev-main",
    "phico/filesystem": "dev-main",
    "aws/aws-sdk-php": "^3.317"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.11",
    "pestphp/pest": "^2.34"
  }
}
